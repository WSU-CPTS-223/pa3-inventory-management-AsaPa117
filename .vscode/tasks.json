{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build pa3 project",
      "type": "shell",
      "command": "/usr/bin/g++",
      "args": [
        "-std=c++17",
        "-g",
        "-O0",
        "-fno-omit-frame-pointer",
        "-fsanitize=address,undefined",
        "main.cpp",
        "data.cpp",
        "Data_Struct_Wrap.cpp",
        "-o",
        "pa3exe"
      ],
      "options": {
        "cwd": "${workspaceFolder}/pa3"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}